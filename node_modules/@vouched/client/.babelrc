{
 "presets": [
    ["env", { "targets": { "browsers": [ "> 1%", "last 2 versions" ] } } ],
  ],
  "plugins": [
    [
      "transform-object-rest-spread",
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "client": "./src/client",
          "tests": "./tests",
          "common": "./src/common",
          "config": "./src/config",
        },
      }
    ]
  ]
}