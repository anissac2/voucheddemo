{
  "parser": "babel-eslint",
  "extends": [
    "airbnb-base",
    "prettier",
    "plugin:react/recommended"
  ],
  "plugins": [
    "jest",
    "react"
  ],
  "env": {
    "node": true,
    "jest/globals": true
  },
  "globals": {
    "window": true,
    "document": true,
    "Blob": true,
    "FileReader": true,
    "BarcodeReader": true,
    "MediaStreamTrack": true,
    "requestAnimationFrame": true,
    "navigator":true,
    "Image": true,
    "cv": true,
    "alert": true,
    "screen": true,
    "faceapi": true,
    "FormData": true
  },
  "parserOptions": {
    "ecmaVersion": 8
  },
  "rules": {
    "max-len": ["error", 80],
    "arrow-body-style": 0,
    "import/no-unresolved": 0,
    "import/no-webpack-loader-syntax": 0,
    "class-methods-use-this": 0,
    "no-alert": 0,
    "no-console": 0,
    "no-underscore-dangle": 0,
    "camelcase": 0,
    "no-param-reassign": 0,
    "import/extensions": 0,
    "import/prefer-default-export": 0,
    "quotes": [2, "single"],
    "import/no-extraneous-dependencies": 0,
    "no-unused-expressions": 1
  }
}
